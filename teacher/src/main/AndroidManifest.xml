<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tongxun.atongmu.teacher">

    <application
        android:name=".application.TeacherApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity" />
        <activity
            android:name=".ui.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.notice.NoticeNewsActivity" />
        <activity android:name=".ui.notice.NoticeListActivity" />
        <activity android:name=".ui.WebViewActivity" />
        <activity android:name=".ui.notice.NoReadActivity" />
    </application>

</manifest>